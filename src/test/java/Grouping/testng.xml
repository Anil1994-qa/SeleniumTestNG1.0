<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Grouping Suite">
<groups>
<run>
<!--include name="sanity"/>-->
<include name="Function"/>
<exclude name="Regression"/>
</run>
</groups>
  <test thread-count="5" name="Test">
    <classes>
      <class name="Grouping.SanityGroup"/>
      <class name="Grouping.SanityNFunction"/>
      <class name="Grouping.RegressionGroup"/>
      <class name="Grouping.FuntionalGroup"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
